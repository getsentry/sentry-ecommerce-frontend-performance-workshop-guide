{
  "name": "sentry-build",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write . --ignore-path .gitignore",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "lint": "eslint . --ext .js,.ts,.astro,.mdx --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.ts,.astro,.mdx --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@astrojs/starlight": "^0.32.1",
    "@astrojs/vercel": "^8.1.1",
    "@sentry/astro": "^9.27.0",
    "astro": "^5.11.1",
    "sharp": "^0.32.6"
  },
  "packageManager": "pnpm@10.18.1",
  "devDependencies": {
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-mdx": "^3.6.2",
    "prettier": "^3.6.2"
  }
}
